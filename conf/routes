GET     /datatable2             controllers.MainController.datatable2()
GET     /page1      	        controllers.MainController.page1()
GET     /page2      	        controllers.MainController.page2()
GET     /newRegister   	        controllers.MainController.newRegister()
GET     /createUser             controllers.MainController.createUser()
GET		/computers		  		controllers.MainController.computers(callback:String)
GET		/computersPDF			controllers.MainController.computersPDF()
GET		/computersJson	  		controllers.MainController.computersJson()
GET		/computers/update		controllers.MainController.update(callback:String, computer:models.Computer)
GET		/computers/delete		controllers.MainController.delete(callback:String, computer:models.Computer)
GET		/computers/create		controllers.MainController.create(callback:String, computer:models.Computer)
GET		/template4				controllers.MainController.template4()

GET     /assets/*file           controllers.Assets.at(path="/public", file)
