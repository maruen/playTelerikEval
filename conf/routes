GET     /                           	controllers.MainController.index()
GET     /createUser                    	controllers.MainController.createUser()
GET     /datatable1                		controllers.MainController.datatable1()
GET     /telerikGrid               		controllers.MainController.telerikGrid()
GET     /page1      	          		controllers.MainController.page1()
GET     /page2      	          		controllers.MainController.page2()
GET     /newRegister   	          		controllers.MainController.newRegister()
GET		/telerik/computers		  		controllers.MainController.computers(callback:String)
GET		/telerik/computersJson	  		controllers.MainController.computersJson()
GET		/telerik/computers/update		controllers.MainController.update(callback:String, computer:models.Computer)
GET		/telerik/computers/delete		controllers.MainController.delete(callback:String, computer:models.Computer)
GET		/telerik/computers/create		controllers.MainController.create(callback:String, computer:models.Computer)
GET     /assets/*file               	controllers.Assets.at(path="/public", file)
